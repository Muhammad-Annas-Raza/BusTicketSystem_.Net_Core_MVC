<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>SRC / Home</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="~/BusOwnerassets/img/logo.png" rel="icon">
  <link href="~/BusOwnerassets/img/logo.png" rel="apple-touch-icon">
  

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="~/BusOwnerassets/vendor/font.css">


  <!-- Vendor CSS Files -->
  <link href="~/BusOwnerassets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="~/BusOwnerassets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="~/BusOwnerassets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="~/BusOwnerassets/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="~/BusOwnerassets/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="~/BusOwnerassets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="~/BusOwnerassets/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="~/BusOwnerassets/css/style.css" rel="stylesheet">


</head>


<body onload="myfun()">
    <div id="preloader" ></div>

  <!-- ======= Header ======= -->
  <header id="header" class="header fixed-top d-flex align-items-center">

    <div class="d-flex align-items-center justify-content-between">
      <a href="#" class="logo d-flex align-items-center">
        <img src="~/BusOwnerassets/img/logo.png" alt="">
        <span class="d-none d-lg-block">Travel Agencies</span>
      </a>
      <i class="bi bi-list toggle-sidebar-btn"></i>
    </div><!-- End Logo -->

    <!-- <div class="search-bar">
      <form class="search-form d-flex align-items-center" method="POST" action="#">
        <input type="text" name="query" placeholder="Search" title="Enter search keyword">
        <button type="submit" title="Search"><i class="bi bi-search"></i></button>
      </form>
    </div>
     -->
    <!-- End Search Bar -->
    <nav class="header-nav ms-auto">
      <ul class="d-flex align-items-center">
        <li class="nav-item d-block d-lg-none">
          <a class="nav-link nav-icon search-bar-toggle " href="#">
            <i class="bi bi-search"></i>
          </a>
        </li><!-- End Search Icon-->     
        <li class="nav-item dropdown pe-3">
          <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">             
            <span class="d-none d-md-block dropdown-toggle ps-2">Hi, @context.HttpContext.Session.GetString("Name")</span>
          </a><!-- End Profile Iamge Icon -->
          <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
            <li class="dropdown-header">
              <h6>@context.HttpContext.Session.GetString("Name")</h6>
              <span>@context.HttpContext.Session.GetString("Role")</span>
            </li>
            <li>
              <hr class="dropdown-divider">
            </li>
            <li>
              <a class="dropdown-item d-flex align-items-center" asp-action="Profile" asp-controller="SuperAdmin">
                <i class="bi bi-person"></i>               
                <span>My Profile</span>
              </a>
            </li>
             <li>
              <hr class="dropdown-divider">
            </li>
            <li>
              <a class="dropdown-item d-flex align-items-center" asp-action="OrganizationProfile" asp-controller="BusOwner">
                 <img src="~/BusOwnerassets/img/bus_profile.png" height="18" style="margin-right:10px;"/>
                <span>Organization Profile</span>
              </a>
            </li>
            <li>
              <hr class="dropdown-divider">
            </li>
            <li>
              <a class="dropdown-item d-flex align-items-center" asp-controller="Home" asp-action="Logout">
                <i class="bi bi-box-arrow-right"></i>
                <span>Sign Out</span>
              </a>
            </li>
          </ul><!-- End Profile Dropdown Items -->
          <!-- End Profile Dropdown Items -->
        </li><!-- End Profile Nav -->
      </ul>
    </nav><!-- End Icons Navigation -->
  </header><!-- End Header -->
  <!-- ======= Sidebar ======= -->
 <aside id="sidebar" class="sidebar">
  <ul class="sidebar-nav" id="sidebar-nav">
      <li class="nav-item">
                <a class="nav-link collapsed" asp-controller="BusOwner" asp-action="Index">
          <i  class="bx-sm bi bi-grid"></i>
         <span>Dashboard</span>
        </a>
      </li><!-- End Dashboard Nav -->

      <li class="nav-item">
        <a class="nav-link collapsed" asp-controller="BusOwner" asp-action="AddConductor">
          <i class="bx-sm ri-user-add-line"></i>
          <span>Add Conductor</span>
        </a>
      </li><!-- End Add Users Nav -->

      <li class="nav-item">
        <a class="nav-link collapsed"  asp-controller="BusOwner" asp-action="ViewConductor">
          <i class="bx-sm bi bi-people"></i>
          <span>View Conductors</span><i class="ms-auto"></i>
        </a>
        
      </li><!-- End View Users Nav -->

        <li class="nav-item">
        <a class="nav-link collapsed"  asp-controller="BusOwner" asp-action="AddBus">
          <i class='bx bx-plus bx-sm'></i>
          <span>Add Bus</span>
        </a><!-- End Add Categories Nav -->
  
      <li class="nav-item">
        <a class="nav-link collapsed"  asp-controller="BusOwner" asp-action="ViewBus">
          <i class="bx-sm bi bi-eye"></i>
          <span>View Buses</span>
        </a>
      </li><!-- End View Categories Nav -->

   <li class="nav-item">
        <a class="nav-link collapsed" asp-controller="BusOwner" asp-action="ViewDiscount">
          <i class='bx-sm bx bx-add-to-queue'></i>
          <span>Discount</span>
        </a><!-- End Add Categories Nav -->
  
   @*   <li class="nav-item">
        <a class="nav-link collapsed" href="viewcategory.html">
         <i class="bx-sm bi bi-eye-fill"></i>
          <span>View Discount</span>
        </a>
      </li><!-- End View Categories Nav -->*@
 
      <li class="nav-item">
        <a class="nav-link collapsed" asp-controller="BusOwner" asp-action="Sale">
          <i class='bx-sm bi bi-coin' ></i>
          <span>Sales</span>
        </a>
      </li><!-- End Sale Point Nav -->  
      <li class="nav-item">
        <a class="nav-link collapsed" asp-controller="Home" asp-action="Logout">
          <i class="ri-logout-box-line bx-sm"></i>
         <span>Sign out</span>
        </a>
      </li><!-- End Login Page Nav -->  
      </ul>
</aside>


<main id="main" class="main">



          @RenderBody()

</main>




  <!-- ======= Footer ======= -->
  <footer id="footer" class="footer">
    <div class="copyright">
      &copy; Copyright <strong><span>SRC Travel Agencies</span></strong>. All Rights Reserved
    </div>

    <div class="credits">
      Developed by <a href="#">Muhammad Annas Raza</a>
    </div>

  </footer><!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

<script src="~/BusOwnerassets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

<!-- Keep Script files in this order is Must-->

  <!-- Main JS File -->
  <script src="~/BusOwnerassets/js/main.js"></script>

